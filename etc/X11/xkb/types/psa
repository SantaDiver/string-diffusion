partial default xkb_types "default" {
virtual_modifiers LevelThree;
type "letters_on_number_row" {
modifiers = Shift+Lock+LevelThree;
map[None] = Level1;           map[Shift] = Level2;
map[LevelThree] = Level3;     map[Shift+LevelThree] = Level4;
map[Lock]  = Level2;          map[Lock+Shift] = Level5;
map[Lock+LevelThree] = Level3;map[Lock+Shift+LevelThree] = Level4;
level_name[Level1] = "Base";  level_name[Level2] = "Shift";
level_name[Level3] = "AltGr"; level_name[Level4] = "Shift AltGr";
level_name[Level5] = "Shift Lock";
}; };
